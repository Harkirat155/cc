{
    "name": "crisscross-tictactoe",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "homepage": "https://harkirat155.github.io/crissCross",
    "description": "A modern, minimalist Tic Tac Toe game built with React and Tailwind CSS.",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "serve": "vite preview",
        "server": "node server/app.js",
        "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
        "lint": "eslint . --cache",
        "lint:fix": "eslint . --fix",
        "lint:frontend": "eslint src --ext .js,.jsx",
        "lint:backend": "eslint server --ext .js",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "check": "npm run lint && npm run build && node --check server/app.js"
    },
    "dependencies": {
        "@radix-ui/react-tooltip": "^1.2.8",
        "compression": "^1.8.1",
        "express": "^5.1.0",
        "lucide-react": "^0.544.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^7.9.1",
        "simple-peer": "^9.11.1",
        "socket.io": "^4.7.5",
        "socket.io-client": "^4.7.5"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.0",
        "@babel/preset-react": "^7.27.1",
        "@eslint/js": "^9.33.0",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@vitejs/plugin-react": "^4.7.0",
        "autoprefixer": "^10.4.0",
        "concurrently": "^8.2.2",
        "eslint": "^9.33.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "jsdom": "^26.1.0",
        "postcss": "^8.4.0",
        "tailwindcss": "^3.3.0",
        "vite": "^4.0.0",
        "vitest": "^1.6.1"
    }
}
